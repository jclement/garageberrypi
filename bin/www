#!/usr/bin/env node
var debug = require('debug')('garageberrypi');
var garageberrypi = require('../garageberrypi');

garageberrypi.app.set('port', process.env.PORT || 3000);

var server = garageberrypi.http.listen(garageberrypi.app.get('port'), function() {
  debug('Express server listening on port ' + server.address().port);
});
